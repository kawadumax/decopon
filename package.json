{
  "name": "decopon",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "web:generate:routes": "pnpm -F web generate:routes",
    "web:build": "pnpm -F core gen-routes && pnpm -F web build",
    "web:build:dry": "pnpm -F core gen-routes && pnpm -F web build:dry",
    "web:dev": "pnpm -F web dev",
    "core:storybook": "pnpm -F core storybook",
    "app:build": "pnpm -F app tauri build",
    "app:dev": "pnpm -F app tauri dev"
  },
  "packageManager": "pnpm@10.11.0",
  "dependencies": {
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "devDependencies": {
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@types/node": "^22.15.21",
    "@biomejs/biome": "^1.9.4",
    "concurrently": "^9.1.2",
    "depcheck": "^1.4.7",
    "typescript": "catalog:",
    "esbuild": "^0.25.2",
    "tailwindcss": "catalog:"
  }
}
