{
  "name": "decopon",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "windows:dev": "pnpm dlx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"pnpm backend:dev\" \"pnpm web:dev\" --names=backend,web",
    "backend:dev": "cd backend && php artisan serve --host=0.0.0.0 --port=8000",
    "backend:fresh": "cd backend && php artisan migrate:fresh --seed",
    "web:generate:routes": "pnpm -F web generate:routes",
    "web:build": "pnpm -F core gen-routes && pnpm -F web build",
    "web:build:dry": "pnpm -F core gen-routes && pnpm -F web build:dry",
    "web:dev": "pnpm -F web dev",
    "core:storybook": "pnpm -F core storybook",
    "app:build": "pnpm -F app tauri build",
    "app:dev": "pnpm -F app tauri dev"
  },
  "packageManager": "pnpm@10.11.0",
  "dependencies": {
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "devDependencies": {
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@types/node": "^22.15.21",
    "@biomejs/biome": "^1.9.4",
    "concurrently": "^9.1.2",
    "depcheck": "^1.4.7",
    "typescript": "catalog:",
    "esbuild": "^0.25.2",
    "tailwindcss": "catalog:"
  }
}
