{
  "compilerOptions": {
    "paths": {
      "@/*": [
        "../core/dist/*",
        "../../core/dist/*",
        "../core/src/*",
        "../../core/src/*"
      ],
      "@lib/*": [
        "../core/dist/scripts/lib/*",
        "../../core/dist/scripts/lib/*",
        "../core/src/scripts/lib/*",
        "../../core/src/scripts/lib/*"
      ],
      "@components/*": [
        "../core/dist/scripts/components/*",
        "../../core/dist/scripts/components/*",
        "../core/src/scripts/components/*",
        "../../core/src/scripts/components/*"
      ],
      "@pages/*": [
        "../core/dist/scripts/pages/*",
        "../../core/dist/scripts/pages/*",
        "../core/src/scripts/pages/*",
        "../../core/src/scripts/pages/*"
      ],
      "@public/*": ["../core/public/*", "../../core/public/*"],
      "@hooks/*": [
        "../core/dist/scripts/hooks/*",
        "../../core/dist/scripts/hooks/*",
        "../core/src/scripts/hooks/*",
        "../../core/src/scripts/hooks/*"
      ],
      "@store/*": [
        "../core/dist/scripts/store/*",
        "../../core/dist/scripts/store/*",
        "../core/src/scripts/store/*",
        "../../core/src/scripts/store/*"
      ],
      "@decopon/core": [
        "../core/dist",
        "../../core/dist",
        "../core/src",
        "../../core/src"
      ],
      "@decopon/core/*": [
        "../core/dist/*",
        "../../core/dist/*",
        "../core/src/*",
        "../../core/src/*"
      ]
    }
  }
}
