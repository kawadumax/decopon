{
  name: 'decopon',
  version: '1.0.0',
  private: true,
  scripts: {
    preinstall: 'npx only-allow pnpm',
    build: 'pnpm -F core build && tsc -b tsconfig.build.json',
    'clean:build-artifacts': 'pnpm -r run clean:build-artifacts',
    'fullstack:dev': 'concurrently -c "#93c5fd,#c4b5fd,#fb7185,#fdba74" "pnpm axum:dev:web" "pnpm web:dev" --names=axum,web',
    'fullstack:preview': 'concurrently -c "#93c5fd,#c4b5fd,#fb7185,#fdba74" "pnpm axum:dev:web" "pnpm web:preview" --names=axum,web',
    'axum:dev': 'cd backend/axum && cargo run --bin app',
    'axum:dev:app': 'cd backend/axum && cargo run --bin app',
    'axum:dev:web': 'cd backend/axum && cargo run --bin web --no-default-features --features web',
    'axum:build': 'cd backend/axum && cargo build --bin app',
    'axum:build:app': 'cd backend/axum && cargo build --bin app',
    'axum:build:web': 'cd backend/axum && cargo build --bin web --no-default-features --features web',
    'axum:fresh': 'cd backend/axum && cargo run -p migration -- fresh',
    'axum:gen:entity': 'cd backend/axum && sea-orm-cli generate entity -o src/entities',
    'web:build': 'pnpm -F web build',
    'web:dev': 'pnpm -F web dev',
    'web:preview': 'pnpm -F web preview',
    'core:gen-routes': 'pnpm -F core gen-routes',
    'core:storybook': 'pnpm -F core storybook',
    'lint:i18n': 'node scripts/check-i18n-keys.mjs',
    'measure:colors': 'node scripts/measure-colors.mjs',
    'windows:build': 'pnpm -F core build && pnpm -F @decopon/app-windows tauri build',
    'windows:dev': 'pnpm -F @decopon/app-windows tauri dev',
    'android:build': 'pnpm -F core build && pnpm -F @decopon/app-android tauri android build --target aarch64',
    'preandroid:build': 'pnpm version:sync',
    'android:dev': 'pnpm -F @decopon/app-android tauri android dev',
    'android:halt': 'node scripts/android-halt.mjs',
    'sites:build': 'pnpm -F @decopon/sites build',
    'version:sync': 'node ./scripts/sync-version.mjs',
    'prewindows:build': 'pnpm version:sync'
  },
  packageManager: 'pnpm@10.18.2',
  dependencies: {
    react: 'catalog:',
    'react-dom': 'catalog:',
    zustand: '^4.5.7',
    'unist-util-visit': '^5.0.0',
  },
  devDependencies: {
    '@biomejs/biome': '^1.9.4',
    '@types/node': '^22.15.21',
    '@types/react': 'catalog:',
    '@types/react-dom': 'catalog:',
    concurrently: '^9.1.2',
    depcheck: '^1.4.7',
    esbuild: '^0.25.2',
    tailwindcss: 'catalog:',
    typescript: 'catalog:',
    vite: 'catalog:',
  },
  pnpm: {
    overrides: {
      axios: '^1.12.0',
      'brace-expansion': '2.0.2',
      devalue: '^5.4.2',
      'form-data': '^4.0.4',
      'vite@^5.0.0': '5.4.21',
      'vite@^6.0.0': '6.4.1',
    },
  },
}
